<Window  x:Class="SalesProject.UCSalesCreation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SalesCreation" Height="768" Width="1024"
        xmlns:my="clr-namespace:SalesProject"
        xmlns:local="clr-namespace:DataGridNameSpace"
        WindowStartupLocation="CenterScreen" Name="sales" WindowState="Minimized" WindowStyle="None" FontSize="14" Background="#FF05053B" Loaded="Window_Loaded" Icon="/SalesProject;component/Images/fevicon.ico">   
   

    <Window.Resources>
        
        <RoutedUICommand x:Key="MyCommand1"  />
        <RoutedUICommand x:Key="MyCash"  />
        <RoutedUICommand x:Key="MyNETS"  />
        <RoutedUICommand x:Key="MyStopAtRate"  />        
        <RoutedUICommand x:Key="MyCashDraw"  />
        <RoutedUICommand x:Key="MyUp"  />
        <RoutedUICommand x:Key="MyDown"  />
        <RoutedUICommand x:Key="MyRemove"  />
        <RoutedUICommand x:Key="MyHold"  />
        <RoutedUICommand x:Key="MyVoid"  />
        <RoutedUICommand x:Key="MyPrint"  />
        <RoutedUICommand x:Key="MyMinimize"  />
        
        <RoutedUICommand x:Key="MyTenderExact"  />
        <RoutedUICommand x:Key="MyOne"  />
        <RoutedUICommand x:Key="MyTwo"  />
        <RoutedUICommand x:Key="MyFive"  />
        <RoutedUICommand x:Key="MyTen"  />
        <RoutedUICommand x:Key="MyFifty"  />
        <RoutedUICommand x:Key="MyHundred"  />
        <RoutedUICommand x:Key="MyFifteen"  />
        <RoutedUICommand x:Key="MyTwenty"  />
        <RoutedUICommand x:Key="MyCreditCard"  />
        <RoutedUICommand x:Key="MyClear"  />
        <RoutedUICommand x:Key="MyHouseAc" />
        <RoutedUICommand x:Key="MyVoucher" />

        <Style x:Key="fie" TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>                   
                        <ControlTemplate TargetType="{x:Type Button}">
                        <Border Padding="5" Margin="3,3,2,2"  BorderThickness="1"  BorderBrush="#FFF2F2F8"   SnapsToDevicePixels="True" Background="#373599B9"  >
                            <TextBlock Text="{TemplateBinding Content}" HorizontalAlignment="Center"  TextWrapping="Wrap"/>
                        </Border>
                    </ControlTemplate>                        
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="btnGroup" TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Padding="5" CornerRadius="10" Margin="2,2,2,2" BorderThickness="1" BorderBrush="#FFF2F2F8"   SnapsToDevicePixels="True" Background="#373599B9"  >
                            <TextBlock Text="{TemplateBinding Content}" Width="100" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"  TextWrapping="Wrap"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="ButtonStyle1" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border x:Name="Chrome" BorderBrush="White" BorderThickness="2" CornerRadius="2" Background="{TemplateBinding Property=Background}">
                            <ContentPresenter  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ButtonStyle2" TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">

                        <Border x:Name="Chrome" BorderBrush="White" BorderThickness="2" CornerRadius="2" Background="{TemplateBinding Property=Background}">
                            <TextBlock Text="{TemplateBinding Content}" FontSize="12" TextWrapping="Wrap" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}"  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            <!--<ContentPresenter  HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>-->
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>

    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource MyCommand1}" Executed="FirstMethod" />
        <CommandBinding Command="{StaticResource MyCash}" Executed="btnCashMethod" />
        <CommandBinding Command="{StaticResource MyNETS}" Executed="btnNETSMethod" />
        <CommandBinding Command="{StaticResource MyStopAtRate}" Executed="StopAtRateMethod" />        
        <CommandBinding Command="{StaticResource MyCashDraw}" Executed="btnCashDrawMethod" />
        <CommandBinding Command="{StaticResource MyUp}" Executed="btnUpMethod" />
        <CommandBinding Command="{StaticResource MyDown}" Executed="btnDownMethod" />
        <CommandBinding Command="{StaticResource MyPrint}" Executed="btnPrintMethod" />
        <CommandBinding Command="{StaticResource MyHold}" Executed="btnHoldMethod" />
        <CommandBinding Command="{StaticResource MyVoid}" Executed="btnVoidMethod" />
        <CommandBinding Command="{StaticResource MyMinimize}" Executed="btnMinimizeMethod" />

        <CommandBinding Command="{StaticResource MyTenderExact}" Executed="btnTenderExactMethod" />       
        <CommandBinding Command="{StaticResource MyOne}" Executed="btnOneMethod" />
        <CommandBinding Command="{StaticResource MyTwo}" Executed="btnTwoMethod" />
        <CommandBinding Command="{StaticResource MyFive}" Executed="btnFiveMethod" />
        <CommandBinding Command="{StaticResource MyTen}" Executed="btnTenMethod" />
        <CommandBinding Command="{StaticResource MyFifty}" Executed="btnFiftyMethod" />
        <CommandBinding Command="{StaticResource MyFifteen}" Executed="btnFifteenMethod" />
        <CommandBinding Command="{StaticResource MyTwenty}" Executed="btnTwentyMethod"/>
        <CommandBinding Command="{StaticResource MyHundred}" Executed="btnHundredMethod" />
        <CommandBinding Command="{StaticResource MyCreditCard}" Executed="btnCreditCardMethod" />
        <CommandBinding Command="{StaticResource MyClear}" Executed="btnClearMethod" />
        <CommandBinding Command="{StaticResource MyHouseAc}" Executed="btnHouseAcMethod" />
        <CommandBinding Command="{StaticResource MyVoucher}" Executed="btnVoucherMethod" />
    </Window.CommandBindings>

    <Window.InputBindings>
        <!--<KeyBinding Key="Z" Modifiers="Ctrl" Command="{StaticResource MyCommand1}" />-->
        <KeyBinding Key="D" Modifiers="Alt" Command="{StaticResource MyCommand1}" />
        <KeyBinding Key="F4"  Command="{StaticResource MyStopAtRate}" />
        <KeyBinding Key="F5"  Command="{StaticResource MyCash}" />
        <KeyBinding Key="F6"  Command="{StaticResource MyNETS}" />
        <KeyBinding Key="UP"  Command="{StaticResource MyUp}" />
        <KeyBinding Key="Down"  Command="{StaticResource MyDown}" />
        <KeyBinding Key="F8"  Command="{StaticResource MyHold}" />
        <KeyBinding Key="F3"  Command="{StaticResource MyVoid}" />
        <KeyBinding Key="F9"  Command="{StaticResource MyPrint}" />
        <KeyBinding Key="DELETE"  Command="{StaticResource MyRemove}" />
        <KeyBinding Key="F10"  Command="{StaticResource MyCashDraw}" />
        <KeyBinding Key="F12"  Command="{StaticResource MyMinimize}" />

        <KeyBinding Key="D1" Modifiers="Alt" Command="{StaticResource MyTenderExact}" />
        <KeyBinding Key="D2" Modifiers="Alt" Command="{StaticResource MyOne}" />
        <KeyBinding Key="D3" Modifiers="Alt" Command="{StaticResource MyTwo}" />
        <KeyBinding Key="D4" Modifiers="Alt" Command="{StaticResource MyFive}" />
        <KeyBinding Key="D5" Modifiers="Alt" Command="{StaticResource MyTen}" />
        <KeyBinding Key="D6" Modifiers="Alt" Command="{StaticResource MyFifty}" />
        <KeyBinding Key="D7" Modifiers="Alt" Command="{StaticResource MyHundred}" />
        <KeyBinding Key="D8" Modifiers="Alt" Command="{StaticResource MyFifteen}" />
        <KeyBinding Key="D9" Modifiers="Alt" Command="{StaticResource MyTwenty}" />
        <KeyBinding Key="B" Modifiers="Alt" Command="{StaticResource MyCreditCard}" />
        <KeyBinding Key="C" Modifiers="Alt" Command="{StaticResource MyClear}" />
        <KeyBinding Key="H" Modifiers="Alt" Command="{StaticResource MyHouseAc}" />
        <KeyBinding Key="V" Modifiers="Alt" Command="{StaticResource MyVoucher}" />

    </Window.InputBindings>
